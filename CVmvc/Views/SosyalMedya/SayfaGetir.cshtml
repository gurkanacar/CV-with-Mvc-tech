@model  CVmvc.Models.Entity.TblSosyalMedya


@{
    ViewBag.Title = "SayfaGetir";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}



<h2>Sosyal Medya Hesap Duzenleme</h2>
<br />

<form method="post">
    @Html.Label("Sosyal Medya Adi")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("Icon")
    @Html.TextBoxFor(x => x.icon, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Update</button>

</form>

